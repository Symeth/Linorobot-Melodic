<launch>
  
  <node pkg="ar_tag_toolbox" name="ar_follower" type="ar_tag_tracker.py" clear_params="true" output="screen">
    <rosparam>
       rate: 10
       max_x: 20.0
       goal_x: 0.4
       x_threshold: 0.05
       y_threshold: 0.05
       y_scale: -1.0
       x_scale: -0.5
       max_angular_speed: 2.0
       min_angular_speed: 0.1
       max_linear_speed: 0.3
       min_linear_speed: 0.1
    </rosparam>
    
  </node>
</launch>
